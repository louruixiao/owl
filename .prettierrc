{
	"printWidth": 160,
	"tabWidth": 1,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"htmlWhitespaceSensitivity": "ignore",
	"trailingComma": "none",
	"useTabs": true,
	"jsxSingleQuote": false,
	"arrowParens": "always",
	"rangeStart": 0,
	"proseWrap": "always",
	"endOfLine": "lf"
}
