	"license": "MIT",
	"version": "0.0.3",
	"private": false,
	"types": "dist/owl.d.ts",
	"files": [
		"dist"
	],
	"main": "./dist/owl.umd.js",
	"module": "./dist/owl.es.js",
	"exports": {
		".": {
			"require": "./dist/owl.umd.js",
			"import": "./dist/owl.es.js"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"serve": "vite preview",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:serve": "vitepress serve docs",
		"commit": "cz",
		"relase": "standard-version",
		"build:style": "webpack --config build/webpack.style.ts",
		"build:u": "NODE_ENV=production webpack --config build/webpack.umd.ts",
		"build:c": "NODE_ENV=production webpack --config build/webpack.cjs.ts",
		"build:e": "vue-tsc --noEmit && vite build",
		"build:all": "NODE_ENV=production webpack --config build/webpack.umd.ts && webpack --config build/webpack.cjs.ts && vue-tsc --noEmit && vite build"
	},
	"dependencies": {
		"@fortawesome/fontawesome-pro": "^6.0.0-beta3",
