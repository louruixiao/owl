		"@fortawesome/vue-fontawesome": "^3.0.0-5",
		"core-js": "3",
		"lodash-es": "^4.17.21",
		"vue": "^3.2.25"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.14.7",
		"@babel/preset-typescript": "^7.14.5",
		"@phenomnomnominal/tsquery": "^4.1.1",
